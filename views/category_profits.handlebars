<div class="cols-xs-10 shifter">
	<h2>Category Profits</h2>
	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<th>Category</th> <th>Profit</th>
			</tr>
		</thead>
	
		{{#each data}}
			<tr>
				<td>{{category}}</td><td>R{{profits}}</td>
			</tr>
		{{/each}}

	</table>
</div>